#NA_wiz_layer.off
  #NA_wiz_layer_top
    Nouvel abonnement
  #NA_wiz_layer_milieu
    #NA_wiz_menu
      %a{ :onclick => "()" } 
        #NA_PB_page_information{ :style => "z-index: 100; margin-left:0px;" }
          Liste des abonnements
      %a{ :onclick => "()" } 
        #NA_PB_page_fichier{ :style => "z-index: 99; margin-left:220px" }
          Détail de l'abonnement
      %a{ :onclick => "()" } 
        #NA_PB_page_resume{ :style => "z-index: 98; margin-left:440px" }
          Résumé
        
    / page information
    #NA_page_information
      .NM_bleu{ :style => "width: 600px; height: 20px;  margin: 10px 0px 0 63px;" }
        Choisissez un abonnement dans la liste
      = render :partial => "sc_admin_detail_workspace/abonnement/abonnement_nouveau_list"
    / page fichier et description
    #NA_page_fichier
      /%div{:style => "width: 7px; height: 330px; float:left;" }
      .NM_bleu{ :style => "width: 600px; height: 20px;  margin: 10px 0px 0 63px;" }
        Abonnement selectionné : 
      .gras#abonnement_name.NM_bleu{ :style => "width: 600px; height: 20px;  margin: 0px 0px 0 63px;" }
        nom
      .zoneText1
        %table{ :style => "margin: 0px 0px 0 10px;" }
          %tr
            %td.cadre{ :style => "width: 200px;" }
              Espace mémoire
            %td.cadre{ :style => "width: 200px;" }
              Double identification
            %td.cadre{ :style => "width: 200px;" }
              prix (Euros)
          %tr
            %td.gras#abonnement_Assigned_memory
              100
            %td.gras#abonnement_security_level
              500 
            %td.gras#abonnement_price
              900
 
    / page résumé
    #NA_page_resume
      .NM_bleu{ :style => "width: 730px; height: 40px;  margin: 0px 0px 0 3px;" }
      .NM_fond_resume{ :style => "width: 730px; margin: 0px 0px 0 3px;" }
        .gras#resume_name.NM_bleu{ :style => "width: 600px; height: 20px;  margin: 0px 0px 0 63px;" }
          nom
        .zoneText1
          %table{ :style => "margin: 0px 0px 0 10px;" }
            %tr
              %td.cadre{ :style => "width: 200px;" }
              Espace mémoire
              %td.cadre{ :style => "width: 200px;" }
                Double identification
              %td.cadre{ :style => "width: 200px;" }
                prix (Euros)
            %tr
              %td.gras#resume_abonnement_Assigned_memory
                100
              %td.gras#resume_abonnement_security_level
                500 
              %td.gras#resume_abonnement_price
                900
      
      #new_abonnement_pic_ok.on{:style => "width: 300px; margin: 20px 0px 0 213px; float:left;" }
        .wiz_pic_ok
        .NM_bleu{ :style => "width: 200px; height:20px; margin: 25px 0px 25px 20px;" }
          Nouvel abonnement validé
      #new_abonnement_pic_wait.off{:style => "width: 300px; margin: 20px 0px 0 213px; float:left;" }
        .wiz_pic_wait
        .NM_bleu{ :style => "width: 200px; height:20px; margin: 25px 0px 25px 20px;" }
          Nouvel abonnement en cours de validation

  #NA_wiz_layer_bas
    #NA_wiz_annul.on{:style => "float: left;" }
      %a{ :style => "cursor: pointer;", :onclick => "displayNewAbonnement('off')" } 
        .New_create_bouton_jaune
          Annuler
    #NA_wiz_suiv.on{:style => "float: right;" }
      %a{ :onclick => "NA_next_stape()", :style => "cursor: pointer;" } 
        .New_footer_bouton_suivant.New_footer_top_bouton{ :style => "margin-right:5px;" }
          Suivant

    #NA_wiz_valid.off{:style => "float: right;" }
      %a{ :onclick => "NA_next_stape()", :style => "cursor: pointer;" } 
        .New_footer_bouton_suivant.New_footer_top_bouton{ :style => "margin-right:5px;" }
          Valider
      %a{ :onclick => "NA_previous_stape()", :style => "cursor: pointer;" } 
        .New_footer_bouton_precedent.New_footer_top_bouton
          Précédent

    #NA_wiz_fin.off{:style => "float: right;" }
      %a{ :onclick => "displayNewAbonnement('off')", :style => "cursor: pointer;" }
        .New_create_bouton_jaune{ :style => "margin-right:5px;" }
          Terminer
