= semantic_form_for [@company, @sc_model] do |f|
  - f.inputs do
    = f.input :dimension, :as => :select, :collection => [["2D", 2], ["3D", 3]], :include_blank => false
    = f.input :name
    = f.input :description
    = f.input :state,       :as => :hidden, :value => "void"
    = f.input :company_id,  :as => :hidden, :value => current_user.company.id

  = f.buttons